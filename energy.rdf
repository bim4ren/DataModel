<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.bim4ren.eu/energymodel#"
     xml:base="http://www.bim4ren.eu/energymodel"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:buildings="http://www.bim4ren.eu/buildings#">
    <owl:Ontology rdf:about="http://www.bim4ren.eu/energymodel">
        <owl:imports rdf:resource="http://www.bim4ren.eu/buildingcomponents"/>
        <owl:imports rdf:resource="http://www.bim4ren.eu/buildings"/>
        <owl:imports rdf:resource="http://www.bim4ren.eu/buildingsystems"/>
        <dc:creator xml:lang="en">Nathalie Charbel</dc:creator>
        <dc:creator xml:lang="en">Pierre Bourreau</dc:creator>
        <dc:description xml:lang="en">This ontology aggregates ontologies to dscribe a building, its components and systems, so as to build up an energy model for the building. It therefore compiles all information, properties and concepts required to build up an energy model for a given building (excluding geometry).</dc:description>
        <dc:title xml:lang="en">Energy model - BIM4Ren project</dc:title>
    </owl:Ontology>
    


